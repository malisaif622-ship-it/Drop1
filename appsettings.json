{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "DefaultConnection": "Your Connection String"
  },

  "Auth": {
    "Mode": "DEV_PASS", // Change to "AD" if using Active Directory later
    "DevPassword": "Your Password",
    "Domain": "MYDOMAIN" // Only used if Mode = "AD"
  },
  "StorageSettings": {
    "RootPath": "Your Directory"
  },

  "Session": {
    "IdleTimeoutMinutes": 30
  }
}
